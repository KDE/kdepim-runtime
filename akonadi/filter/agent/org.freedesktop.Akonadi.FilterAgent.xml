<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.freedesktop.Akonadi.FilterAgent">
    <method name="enumerateMimeTypes">
      <arg type="as" direction="out"/>
    </method>
    <method name="enumerateFilters">
      <arg name="mimeType" type="s" direction="in"/>
      <arg type="as" direction="out"/>
    </method>
    <method name="createFilter">
      <arg name="filterId" type="s" direction="in"/>
      <arg name="mimeType" type="s" direction="in"/>
      <arg name="source" type="s" direction="in"/>
      <arg type="i" direction="out"/>
    </method>
    <method name="deleteFilter">
      <arg name="filterId" type="s" direction="in"/>
      <arg type="i" direction="out"/>
    </method>
    <method name="attachFilter">
      <arg name="filterId" type="s" direction="in"/>
      <arg name="attachedCollectionIds" type="ax" direction="in"/>
      <annotation name="com.trolltech.QtDBus.QtTypeName.In1" value="QList&lt;Akonadi::Collection::Id&gt;"/>
      <arg type="i" direction="out"/>
    </method>
    <method name="detachFilter">
      <arg name="filterId" type="s" direction="in"/>
      <arg name="detachedCollectionIds" type="ax" direction="in"/>
      <annotation name="com.trolltech.QtDBus.QtTypeName.In1" value="QList&lt;Akonadi::Collection::Id&gt;"/>
      <arg type="i" direction="out"/>
    </method>
    <method name="getFilterProperties">
      <arg name="filterId" type="s" direction="in"/>
      <arg type="i" direction="out"/>
      <arg name="mimeType" type="s" direction="out"/>
      <arg name="source" type="s" direction="out"/>
      <arg name="attachedCollectionIds" type="ax" direction="out"/>
      <annotation name="com.trolltech.QtDBus.QtTypeName.Out3" value="QList&lt;Akonadi::Collection::Id&gt;"/>
    </method>
    <method name="changeFilter">
      <arg name="filterId" type="s" direction="in"/>
      <arg name="source" type="s" direction="in"/>
      <arg name="attachedCollectionIds" type="ax" direction="in"/>
      <annotation name="com.trolltech.QtDBus.QtTypeName.In2" value="QList&lt;Akonadi::Collection::Id&gt;"/>
      <arg type="i" direction="out"/>
    </method>
  </interface>
</node>
