<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.Akonadi.Resource">
    <signal name="statusChanged">
      <arg name="status" type="i" direction="out"/>
      <arg name="message" type="s" direction="out"/>
    </signal>
    <signal name="progressChanged">
      <arg name="progress" type="u" direction="out"/>
      <arg name="message" type="s" direction="out"/>
    </signal>
    <signal name="nameChanged">
      <arg name="name" type="s" direction="out"/>
    </signal>
    <method name="quit">
    </method>
    <method name="status">
      <arg type="i" direction="out"/>
    </method>
    <method name="statusMessage">
      <arg type="s" direction="out"/>
    </method>
    <method name="progress">
      <arg type="u" direction="out"/>
    </method>
    <method name="progressMessage">
      <arg type="s" direction="out"/>
    </method>
    <method name="requestItemDelivery">
      <arg type="b" direction="out"/>
      <arg name="uid" type="i" direction="in"/>
      <arg name="remoteId" type="s" direction="in"/>
      <arg name="type" type="i" direction="in"/>
    </method>
    <method name="configure">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="synchronize">
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="setName">
      <arg name="name" type="s" direction="in"/>
    </method>
    <method name="name">
      <arg type="s" direction="out"/>
    </method>
    <method name="cleanup">
    </method>
    <method name="isOnline">
      <arg type="b" direction="out"/>
    </method>
    <method name="setOnline">
      <arg name="state" type="b" direction="in"/>
    </method>
  </interface>
</node>
