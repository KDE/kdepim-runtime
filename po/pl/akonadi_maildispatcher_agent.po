#: maildispatcheragent.cpp:80 maildispatcheragent.cpp:224
#, kde-format
msgid "Sending messages (1 item in queue)…"
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "Wysyłanie wiadomości (%1 rzecz w w kolejce)…"
msgstr[1] "Wysyłanie wiadomości (%1 rzeczy w w kolejce)…"
msgstr[2] "Wysyłanie wiadomości (%1 rzeczy w w kolejce)…"

#: maildispatcheragent.cpp:90
#, kde-format
msgid "Sending canceled."
msgstr "Porzucono wysyłanie."

#: maildispatcheragent.cpp:97
#, kde-format
msgid "Finished sending messages."
msgstr "Ukończono wysyłanie wiadomości."

#: maildispatcheragent.cpp:103
#, kde-format
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "Pomyślne wysłano wiadomość"

#: maildispatcheragent.cpp:104
#, kde-format
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "Twoja wiadomość została pomyślnie wysłana."

#: maildispatcheragent.cpp:110
#, kde-format
msgid "No items in queue."
msgstr "Brak oczekujących w kolejce."

#: maildispatcheragent.cpp:155
#, kde-format
msgid "Online, sending messages in queue."
msgstr "Online, wysyłanie wiadomości w kolejce."

#: maildispatcheragent.cpp:159
#, kde-format
msgid "Offline, message sending suspended."
msgstr "Offline, wysyłanie wiadomości zawieszone."

#: maildispatcheragent.cpp:184
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "Wysyłanie: %1"

#: maildispatcheragent.cpp:249
#, kde-format
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "Nieudane wysyłanie wiadomości"

#: maildispatcheragent.cpp:282
#, kde-format
msgid "Ready to dispatch messages."
msgstr "Gotowy do wysłania wiadomości."

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "Id zbioru skrzynki wyjściowej"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "Id zbioru wysłanej poczty"

#: outboxqueue.cpp:265
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "Nie można uzyskać dostępu do skrzynki wysłanych wiadomości (%1)."

#: sendjob.cpp:49 sendjob.cpp:50
#, kde-format
msgid "Message sending aborted."
msgstr "Porzucono wysyłanie wiadomości."

#: sendjob.cpp:59
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr ""
"Nie można rozpocząć transportu wiadomości. Prawdopodobnie nieprawidłowy "
"transport."

#: sendjob.cpp:65
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "Nie można wysłać wiadomości. Nieprawidłowy transport."

#: sendjob.cpp:97
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "Pobieranie interfejsu D-Bus zasobu %1 nie powiodło się."

#: sendjob.cpp:110
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "Niepoprawna odpowiedź D-Bus z zasobu %1."

#: sendjob.cpp:229
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr ""
"Domyślny katalog poczty wysłanej jest niedostępny. Przetrzymywanie "
"wiadomości w skrzynce wyjściowej."

#: sendjob.cpp:241
#, kde-format
msgid "Message transport aborted."
msgstr "Porzucono transport wiadomości."

#: sendjob.cpp:241
#, kde-format
msgid "Failed to transport message."
msgstr "Nie można wysłać wiadomości."

#: sendjob.cpp:283
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "Nieudane uzyskanie interfejsu D-Bus dla agenta filtrującego pocztę."

#: sendjob.cpp:290
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "Nieprawidłowa odpowiedź D-Bus od agenta filtrującego pocztę"

#: sendjob.cpp:332
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr ""
"Wysyłanie powiodło się, lecz nie udało się usunięcie wiadomości ze skrzynki "
"wychodzących."

#: sendjob.cpp:335
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr ""
"Wysyłanie powiodło się, lecz nie udało się przeniesienia wiadomości do "
"skrzynki wysłanej poczty."

#: sendjob.cpp:371
#, kde-format
msgid "Failed to store result in item."
msgstr "Zapisanie wyniku w elemencie nie powiodło się."

#: storeresultjob.cpp:59
#, kde-format
msgid "Failed to fetch item."
msgstr "Pobieranie elementu nie powiodło się."

