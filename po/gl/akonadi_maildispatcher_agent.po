#: maildispatcheragent.cpp:80 maildispatcheragent.cpp:224
#, kde-format
msgid "Sending messages (1 item in queue)…"
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "Estanse a enviar mensaxes (1 elemento na fila)…"
msgstr[1] "Estanse a enviar mensaxes (%1 elementos na fila)…"

#: maildispatcheragent.cpp:90
#, kde-format
msgid "Sending canceled."
msgstr "Cancelouse o envío."

#: maildispatcheragent.cpp:97
#, kde-format
msgid "Finished sending messages."
msgstr "Rematou o envío de mensaxes."

#: maildispatcheragent.cpp:103
#, kde-format
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "Enviouse a mensaxe"

#: maildispatcheragent.cpp:104
#, kde-format
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "A mensaxe enviouse correctamente."

#: maildispatcheragent.cpp:110
#, kde-format
msgid "No items in queue."
msgstr "Non hai ningún elemento na fila."

#: maildispatcheragent.cpp:155
#, kde-format
msgid "Online, sending messages in queue."
msgstr "En liña; enviando as mensaxes da fila."

#: maildispatcheragent.cpp:159
#, kde-format
msgid "Offline, message sending suspended."
msgstr "Desconectado; suspendeuse o envío de mensaxes."

#: maildispatcheragent.cpp:184
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "Enviando: %1"

#: maildispatcheragent.cpp:249
#, kde-format
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "O envío de correo electrónico fallou"

#: maildispatcheragent.cpp:282
#, kde-format
msgid "Ready to dispatch messages."
msgstr "Listo para despachar mensaxes."

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "Identificador da colección de Saída"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "Identificador da colección de Correo Enviado"

#: outboxqueue.cpp:265
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "Non foi posíbel acceder ao cartafol de saída (%1)."

#: sendjob.cpp:49 sendjob.cpp:50
#, kde-format
msgid "Message sending aborted."
msgstr "Interrompeuse o envío de mensaxes."

#: sendjob.cpp:59
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr ""
"Non foi posíbel iniciar o transporte de mensaxes. Posibelmente o transporte "
"sexa incorrecto."

#: sendjob.cpp:65
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "Non foi posíbel enviar a mensaxe. O transporte é incorrecto."

#: sendjob.cpp:97
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "Non foi posíbel obter a interface D-Bus do recurso %1."

#: sendjob.cpp:110
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "A resposta D-Bus do recurso %1 é incorrecta."

#: sendjob.cpp:229
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr ""
"O cartafol de correo enviado predeterminado non está dispoñíbel. A mensaxe "
"gardarase na caixa de saída."

#: sendjob.cpp:241
#, kde-format
msgid "Message transport aborted."
msgstr "Interrompeuse o transporte das mensaxes."

#: sendjob.cpp:241
#, kde-format
msgid "Failed to transport message."
msgstr "Non foi posíbel transportar a mensaxe."

#: sendjob.cpp:283
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "Non foi posíbel obter a interface D-Bus do axente de filtro de correo."

#: sendjob.cpp:290
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "A resposta D-Bus do axente de filtro de correo é incorrecta."

#: sendjob.cpp:332
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr ""
"A mensaxe enviouse correctamente, pero non se puido retirar da caixa de "
"saída."

#: sendjob.cpp:335
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr ""
"A mensaxe enviouse correctamente, pero non se puido mover ao cartafol de "
"correo enviado."

#: sendjob.cpp:371
#, kde-format
msgid "Failed to store result in item."
msgstr "Non foi posíbel almacenar o resultado no elemento."

#: storeresultjob.cpp:59
#, kde-format
msgid "Failed to fetch item."
msgstr "Non foi posíbel obter o elemento."

