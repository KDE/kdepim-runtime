#, kde-format
msgctxt "Filedialog filter for *.vcf"
msgid "vCard Address Book File"
msgstr "vCard पतापुस्तिका सञ्चिका"

#: vcardconfig.cpp:24
#, kde-format
msgid "Activities"
msgstr "गतिविधयः"

#: vcardresource.cpp:71
#, kde-format
msgid "Contact with uid '%1' not found."
msgstr "uid '%1' इत्यनेन सह सम्पर्कः न प्राप्तः ।"

#: vcardresource.cpp:168 vcardresource.cpp:188
#, kde-format
msgid "Unable to open vCard file '%1'."
msgstr "vCard सञ्चिकां '%1' उद्घाटयितुं असमर्थम् ।"

#. i18n: ectx: label, entry (Path), group (General)
#: vcardresource.kcfg:10
#, kde-format
msgid "Path to vCard file."
msgstr "vCard सञ्चिकायाः मार्गः ।"

#. i18n: ectx: label, entry (DisplayName), group (General)
#: vcardresource.kcfg:14
#, kde-format
msgid "Display name."
msgstr "नाम प्रदर्शयतु।"

#. i18n: ectx: label, entry (ReadOnly), group (General)
#: vcardresource.kcfg:18
#, kde-format
msgid "Do not change the actual backend data."
msgstr "वास्तविकं पृष्ठभागदत्तांशं न परिवर्तयन्तु ।"

#. i18n: ectx: label, entry (MonitorFile), group (General)
#: vcardresource.kcfg:22
#, kde-format
msgid "Monitor file for changes."
msgstr "परिवर्तनार्थं सञ्चिकायाः निरीक्षणं कुर्वन्तु।"

#. i18n: ectx: label, entry (PeriodicUpdate), group (General)
#: vcardresource.kcfg:26
#, kde-format
msgid "Periodically update the file."
msgstr "समये समये सञ्चिकां अद्यतनं कुर्वन्तु।"

#. i18n: ectx: label, entry (UpdatePeriod), group (General)
#: vcardresource.kcfg:30
#, kde-format
msgid "Automatic update period in minutes."
msgstr "निमेषेषु स्वचालितं अद्यतनकालः।"
