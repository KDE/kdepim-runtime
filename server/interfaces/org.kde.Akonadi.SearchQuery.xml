<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.Akonadi.SearchQuery">
    <method name="start" />

    <method name="stop" />

    <method name="close" />

    <method name="allHits">
      <arg name="iterator" type="s" direction="out" />
    </method>

    <signal name='hitsChanged'>
      <arg name='uris' type='as' />
    </signal>

    <signal name='hitsRemoved'>
      <arg name='uris' type='as' />
    </signal>
  </interface>
</node>
