<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.Akonadi.SearchProvider">
    <signal name="searchResultAvailable">
      <arg name="ticket" type="i" direction="out"/>
    </signal>
    <signal name="fetchResponseAvailable">
      <arg name="ticket" type="i" direction="out"/>
      <arg name="result" type="as" direction="out"/>
    </signal>
    <method name="search">
      <arg type="i" direction="out"/>
      <arg name="targetCollection" type="s" direction="in"/>
      <arg name="searchQuery" type="s" direction="in"/>
    </method>
  </interface>
</node>
