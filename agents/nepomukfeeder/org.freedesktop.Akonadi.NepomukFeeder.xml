<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.freedesktop.Akonadi.NepomukFeeder">
    <method name="updateAll" />
    <method name="forceReindexCollection" >
      <arg name="item" type="x" direction="in"/>
    </method>
    <method name="forceReindexItem" >
      <arg name="item" type="x" direction="in"/>
    </method>
    <method name="disableIdleDetection" >
      <arg name="disable" type="b" direction="in"/>
    </method>
    <method name="isDisableIdleDetection" >
      <arg type="b" direction="out"/>
    </method>
    <method name="queueIsEmpty" >
      <arg type="b" direction="out"/>
    </method>
    <method name="currentCollectionName" >
      <arg type="s" direction="out"/>
    </method>
    <method name="listOfCollection" >
      <arg type="as" direction="out"/>
    </method>
    <method name="totalitems" >
      <arg type="x" direction="out"/>
    </method>
    <method name="indexeditems" >
      <arg type="x" direction="out"/>
    </method>
    <method name="isIndexing" >
      <arg type="b" direction="out"/>
    </method>
  </interface>
</node>
